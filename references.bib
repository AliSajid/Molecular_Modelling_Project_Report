	author = {Nemudryi, Artem and Nemudraia, Anna and Wiegand, Tanner and Surya, Kevin and Buyukyoruk, Murat and Cicha, Calvin and Vanderwood, Karl K. and Wilkinson, Royce and Wiedenheft, Blake},
	month = sep,
