	journal = {Journal of Psychiatric Research},
	author = {Hino, Mizuki and Kunii, Yasuto and Matsumoto, Junya and Wada, Akira and Nagaoka, Atsuko and Niwa, Shin ichi and Takahashi, Hitoshi and Kakita, Akiyoshi and Akatsu, Hiroyasu and Hashizume, Yoshio and Yamamoto, Sakon and Yabe, Hirooki},
	month = nov,
