	note = {Publisher: Blackwell Publishing Inc.},
	keywords = {Inhibitor screening, Inhibitor selectivity, Kinase, Kinase assay, Kinase domain, Small-molecule inhibition},
	pages = {Unit2.9},
}

@article{manning_evolution_2002,
	title = {Evolution of protein kinase signaling from yeast to man},
	volume = {27},
	issn = {09680004},
