	keywords = {CTD, Carboxy-terminal repeat domain, NMR, NTP, Nuclear magnetic resonance, Nucleoside triphosphate, Pol II, RNA polymerase II, RNP, Ribonucleoprotein, Rpb4-Rpb7 heterodimer, Rpb4/7, TATA-box-binding protein, TBP, TF, Transcription factor},
