	keywords = {Animals, Anupreet Kharbanda, Debasmita Saha, Drug Design, Drug Discovery, Extramural, Hong-Yu Li, Humans, Ketones / metabolism*, MEDLINE, Metabolome*, Molecular Conformation*, N.I.H., NCBI, NIH, NLM, National Center for Biotechnology Information, National Institutes of Health, National Library of Medicine, Protein Kinase Inhibitors / chemistry, Protein Kinase Inhibitors / pharmacology, PubMed Abstract, Research Support, Review, Stereoisomerism, doi:10.1021/acs.jmedchem.9b00640, pmid:31550151},
	pages = {441--469},
}

@article{owusu_mapping_2019,
	title = {Mapping the {Human} {Kinome} in {Response} to {DNA} {Damage}},
