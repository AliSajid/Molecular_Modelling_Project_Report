	note = {Publisher: Elsevier},
	keywords = {Animal models, CNS, Computer vision, Drug screening, High-throughput, Machine learning, NeuroCube, PhenoCube, Preclinical research, Smartcube, Social behavior},
	pages = {82--9},
}

